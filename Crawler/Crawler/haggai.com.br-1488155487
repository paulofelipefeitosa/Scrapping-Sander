<script type="text/javascript">
	if( document.location.hostname.indexOf('haggai.com.br') != -1 && document.location.pathname.substr(0,7) != '/videos' ){
		if ( olark ) {
			// olark('api.chat.updateVisitorNickname', { snippet: '' });
