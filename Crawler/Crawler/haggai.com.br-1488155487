			if ( olark_visitor_nickname ) {
				olark('api.chat.updateVisitorNickname', { snippet: olark_visitor_nickname });
			}
			// } else if ( window.jQuery ) {
			// 	window.jQuery.get('//haggai.com.br/cadastro/logged.php', function(data){
