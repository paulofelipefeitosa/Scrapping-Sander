			}

			olark('api.chat.onCommandFromOperator', function(event) {
				if (event.command.name == 'name') {
					olark('api.chat.updateVisitorNickname', { snippet: event.command.body });
				}
				if (event.command.name == 'go') {
					window.location = event.command.body;
				}
			});

			if (olark_hide) {
			  olark('api.box.hide');
			}
		}
	}
