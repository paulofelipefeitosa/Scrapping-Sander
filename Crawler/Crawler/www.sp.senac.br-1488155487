function mergeParm(url,parmName,parmValue){
 var merged=url;
 if( merged.indexOf("&"+parmName+"=")!=-1 ){
 var i1=merged.indexOf("&"+parmName+"=");
 var i2=merged.indexOf("&",i1+parmName.length+2);
 if(i2==-1){
 merged=merged.substring(0,i1)+"&"+parmName+"="+parmValue;
 }else{
 merged=merged.substring(0,i1)+"&"+parmName+"="+parmValue+merged.substring(i2);
 }
 }else if( merged.indexOf("?"+parmName+"=")!=-1 ){
 var i1=merged.indexOf("?"+parmName+"=");
 var i2=merged.indexOf("&",i1+parmName.length+2);
 if(i2==-1){
 merged=merged.substring(0,i1)+"?"+parmName+"="+parmValue;
 }else{
 merged=merged.substring(0,i1)+"?"+parmName+"="+parmValue+merged.substring(i2);
 }
 }else{
 if( merged.indexOf('&')==-1 ){
 merged += "?"+parmName+"="+parmValue;
 }else{
 merged += "&"+parmName+"="+parmValue;
 }
 }
 return merged;
}

function trocaMes(y,m){
 var url = document.location.href;
 url = mergeParm(url,"year",y);
 url = mergeParm(url,"month",m);
 document.location = url; 
}

function getMonthName(i,forced){
 var months=['Janeiro','Fevereiro','Março','Abril','Maio','Junho','Julho','Agosto','Setembro','Outubro','Novembro','Dezembro'];
 var tag = (!forced && monthly_list[i][2]>0 ? 
"<a href=\"javascript:trocaMes('"+monthly_list[i][0]+"','"+monthly_list[i][1]+"')\" class=\"linque1\" id=\"linque1-"+i+"\">" : "<span class=\"calendario-mes\" id=\"linque1-"+i+"\">") +
months[ forced ? i : monthly_list[i][1]-1 ] + 
 (!forced && monthly_list[i][2]>0 ? "</a>" : "</span>");
 return tag;
}

function getStrictMonthName(i,forced){
 var months=['Janeiro','Fevereiro','Março','Abril','Maio','Junho','Julho','Agosto','Setembro','Outubro','Novembro','Dezembro'];
 var tag = months[ forced ? i : monthly_list[i][1]-1 ];
 return tag;
}

function getYear(i){
 return '<span id="year-desc-'+i+'" class="calendario-ano">'+monthly_list[i][0]+'</span>';
}
function setMonthCellBackground(){
 if(parmYear!="" && parmMonth!=""){
 var y=parseInt(parmYear);
 var m=parseInt(parmMonth);
 for(var i=0;i<monthly_list.length;i++){
 if( monthly_list[i][0]==y && monthly_list[i][1]==m ){
 var cellName = "monthcell"+(i+1);
document.getElementById(cellName).style.backgroundColor='#E44F10';

document.getElementById('year-desc-'+i).className='calendario-ano-sel';

 //eval("document.all."+cellName+".style.backgroundColor=\"#E44F10\"");
 }
 }
 }
}

 function getResumedFormatDate(dt){
 if(dt!=null && dt.indexOf('/')!=-1){
 var dts=dt.split('/');
 var d=dts[0];
 var m=dts[1];
 var y=dts[2];
 return (d.charAt(0)=='0'?d.substring(1):d) + '/' + (m.charAt(0)=='0'?m.substring(1):m) + '/' + y;
 }else{
 return '';
 }
 }



function getFormatDateEventos(y1,m1,d1,y2,m2,d2,connector){
 var txt='';
 var _d1=(d1.length<2?'0':'')+d1;
 var _d2=(d2.length<2?'0':'')+d2;
 var _m1=(m1.length<2?'0':'')+m1;
 var _m2=(m2.length<2?'0':'')+m2;
 if(d1!='' && m1!='' && d2!='' && m2!=''){
 if(_m1!=_m2){
 txt = (d1.length<2?'0':'')+d1+' '+getMonthAbbrev(m1) + (y1!=parmYear?' '+y1:'') + ' '+connector.toLowerCase()+' ' + (d2.length<2?'0':'')+d2+' '+getMonthAbbrev(m2) + (y2!=parmYear?' '+y2:'');
 }else if(_d1!=_d2){
 txt = (d1.length<2?'0':'')+d1+ ' '+connector.toLowerCase()+' ' + (d2.length<2?'0':'')+d2+' '+getMonthAbbrev(m2);
 }else{
 txt = (d1.length<2?'0':'')+d1+' '+getMonthAbbrev(m1);
 }
 }else{
 txt = (d1.length<2?'0':'')+d1+' '+getMonthAbbrev(m1);
 }
 return txt.toUpperCase();
}

function getMonthAbbrev(m){
 var months=['JAN','FEV','MAR','ABR','MAI','JUN','JUL','AGO','SET','OUT','NOV','DEZ'];
 var _m=m;
 if(_m.charAt(0)=='0') _m=_m.substring(1);
 return months[ parseInt(_m)-1 ];
}

function getTransformedSiteName(sn){
if(sn=='REDE SENAC SÃO PAULO'){
return 'Rede Senac São Paulo';
}else{
return sn;
}
}

