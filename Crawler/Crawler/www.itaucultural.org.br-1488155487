    <script type='text/javascript'>

        function closeBar(callback, data) {
            height = 0;
            callback = callback || $.noop;

            var bar = $("#wpfront-notification-bar");
            var spacer = $("#wpfront-notification-bar-spacer");

            var fn = callback;
            callback = function() {
                fn();

                if (data.keep_closed_for > 0)
                    $.cookie(keep_closed_cookie, 1, {path: "/", expires: data.keep_closed_for});
                else
                    $.cookie(keep_closed_cookie, 1, {path: "/"});
            };

            //set animation
            if (data.animate_delay > 0) {
                bar.stop().animate({"height": height + "px"}, data.animate_delay * 1000, "swing", callback);
                if (data.fixed_position)
                    spacer.stop().animate({"height": height + "px"}, data.animate_delay * 1000);
            }
            else {
                bar.height(height);
                if (data.fixed_position)
                    spacer.height(height);
                callback();
            }

        }

        $('.question_select').on('change', function(){
            $(".wpfront-message").find("#error").remove();
            if($(this).val() == "10"){
                $(this).parent().append("<div id='outro_motivo_label'>Qual?</div> <input id='outro_motivo' type='text'></input>");
            }else{
                $(this).parent().find("#outro_motivo_label").remove();
                $(this).parent().find("#outro_motivo").remove();
            }
        });

        $(".wpfront-button").on("click", function(){

            if($('.question_select').val() == "0"){
               $(".wpfront-message").append("<div id='error'>Por favor, escolha uma opção!</div>")
               return;
            }

            if($('.question_select').val() == "10" && $("#outro_motivo").val() == ""){
               $(".wpfront-message").append("<div id='error'>Por favor, diga-nos qual o motivo da sua visita!</div>")
               return;
            }

            var nonce = "aab1209595";

            var data = {
                 action: 'audience_interview',
                 audience_motivation: $(".question_select").val(),
                 different_motivation: $("#outro_motivo").val(),
                 security: nonce
            };

            $.post(ajaxurl, data, function(response) {
                if(response.success){
                    closeBar(null, window.data_notification_bar);
                }else{
                    $(".wpfront-message").append("<div id='error'>Ops! tivemos um problema ao salver sua resposta. Você poderia clicar no botão ao lado para enviar novamente?</div>")
                }
            });
        });


    </script>

