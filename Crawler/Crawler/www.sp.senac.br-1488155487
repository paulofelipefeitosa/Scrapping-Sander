var __IS_EVENT=true;

var __CUSTOM_mySize = 12;
var __CUSTOM_maxSize = 20;
var __CUSTOM_minSize = 9;
var __CUSTOM_min=9;
var __CUSTOM_max=20;
var __CUSTOM_FONTSIZECLASS_EVENTS=['intro-paragraph'];

function increaseFontSizeCustom(){
 if(__IS_EVENT){
 for(var i=0;i<__CUSTOM_FONTSIZECLASS_EVENTS.length;i++){
 $('.'+__CUSTOM_FONTSIZECLASS_EVENTS[i]).each(function(index,element){
 var s=0;
 if(element.style.fontSize){
 s=parseInt(element.style.fontSize.replace('px',''));
 }else{
 s=12;
 }
if(s!=__CUSTOM_max) s+=1;
 element.style.fontSize=s+'px';
 }); 
 }
 }else{
 increaseFontSize();
 }
}

function decreaseFontSizeCustom(){
 if(__IS_EVENT){
 for(var i=0;i<__CUSTOM_FONTSIZECLASS_EVENTS.length;i++){
 $('.'+__CUSTOM_FONTSIZECLASS_EVENTS[i]).each(function(index,element){
