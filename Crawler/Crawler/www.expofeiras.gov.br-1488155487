<script>
$(function(){
  $(".icoImprimir").print();
});

$(".icoAum").click(function(){
  var size= parseInt($(".content").css("font-size"));
  if(size < 18) {
    $(".content").css("font-size",size+1)
  }
});

$(".icoRedu").click(function(){
  var size= parseInt($(".content").css("font-size"));
  if(size >14){
    $(".content").css("font-size",size-1)
  }
});

$('a.icoVoltar').click(function(){
    urlVoltar = {"referer":false,"uri":"\/calendario-de-eventos"};
    window.location = urlVoltar.referer;
})

$('.gerarPdf').click(function () {

    var comp = 'resultado(s)';

    if($(".resultadoBusca ul li").html()!= comp){

        window.open($(this).attr('href'),'_blank');
        return false;

    }else{
        return false;
    }
});
</script>




