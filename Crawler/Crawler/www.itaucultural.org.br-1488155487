    <script type="text/javascript">
      jQuery(function() {
        if (!Cookies.get('termos-aceito-2015-11')) {
          jQuery('#marcoCivil').delay(1000).slideDown();
        }

        jQuery('.btn-fechar').click(function(){
          jQuery('#marcoCivil').slideUp();
          Cookies.set('termos-aceito-2015-11', true, { expires: 1 });
        });
      });
    </script>
    <div id="marcoCivil" class="marco-civil-container" style="display:none; height: 36px; color: rgb(255, 255, 255); font-weight: normal; display:;background-color: rgb(87, 87, 87);">
