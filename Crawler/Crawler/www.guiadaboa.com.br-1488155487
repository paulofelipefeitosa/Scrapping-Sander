		 old = splitMapsCoord('-22.9084054,-43.19762079999998,18');		var lat = 0;
		var long = 0;
		var h = mapHeight;

		

		// get coordinates from the map field
		if (old.length>0) {
			lat = parseFloat(old[0]);
			long = parseFloat(old[1]);
			h = parseInt(old[2]);
		} 
		else 
		// try to get the coordinates using geocoding
		{
			var address = " Rio de Janeiro";

			if(address) {
			$.ajax({
				type		: "GET",
				cache		: false,
				url		: "http://www.guiadaboa.com.br/include/get_info.php?type=geocoding&address="+address,
				data		: $(this).serializeArray(),
				success: function(data) {
					if(data) {
 					var split_data=data.split("|");
						lat = split_data[0];
						long = split_data[1];

						// add the point to the map
						p = new google.maps.LatLng(lat , long);
						
						addPoint(p, 'Desfile da Grande Rio - 2017', '260', bounds);
						
						bestFitWithCenter(listings_map, bounds);
					}
				}
			});
			}// end if address

		}

		if(lat && long) {	
		

			p = new google.maps.LatLng( lat , long );
			addPoint(p, 'Desfile da Grande Rio - 2017', '260', bounds);

		} // end if point
	
		
		old=new Array();
		 old = splitMapsCoord('-22.9074455,-43.18308719999999,13');		var lat = 0;
		var long = 0;
		var h = mapHeight;

		

		// get coordinates from the map field
		if (old.length>0) {
			lat = parseFloat(old[0]);
			long = parseFloat(old[1]);
			h = parseInt(old[2]);
		} 
		else 
		// try to get the coordinates using geocoding
		{
			var address = " Rio de Janeiro";

			if(address) {
			$.ajax({
				type		: "GET",
				cache		: false,
				url		: "http://www.guiadaboa.com.br/include/get_info.php?type=geocoding&address="+address,
				data		: $(this).serializeArray(),
				success: function(data) {
					if(data) {
 					var split_data=data.split("|");
						lat = split_data[0];
						long = split_data[1];

						// add the point to the map
						p = new google.maps.LatLng(lat , long);
						
						addPoint(p, 'Toca Rauuul - 2017', '228', bounds);
						
						bestFitWithCenter(listings_map, bounds);
					}
				}
			});
			}// end if address

		}

		if(lat && long) {	
		

			p = new google.maps.LatLng( lat , long );
			addPoint(p, 'Toca Rauuul - 2017', '228', bounds);

		} // end if point
	
		
		old=new Array();
		 old = splitMapsCoord('-22.9084054,-43.19762079999998,17');		var lat = 0;
		var long = 0;
		var h = mapHeight;

		

		// get coordinates from the map field
		if (old.length>0) {
			lat = parseFloat(old[0]);
			long = parseFloat(old[1]);
			h = parseInt(old[2]);
		} 
		else 
		// try to get the coordinates using geocoding
		{
			var address = " Rio de Janeiro";

			if(address) {
			$.ajax({
				type		: "GET",
				cache		: false,
				url		: "http://www.guiadaboa.com.br/include/get_info.php?type=geocoding&address="+address,
				data		: $(this).serializeArray(),
				success: function(data) {
					if(data) {
 					var split_data=data.split("|");
						lat = split_data[0];
						long = split_data[1];

						// add the point to the map
						p = new google.maps.LatLng(lat , long);
						
						addPoint(p, 'Desfile da Imperatriz Leopoldinense - 2017', '262', bounds);
						
						bestFitWithCenter(listings_map, bounds);
					}
				}
			});
			}// end if address

		}

		if(lat && long) {	
		

			p = new google.maps.LatLng( lat , long );
			addPoint(p, 'Desfile da Imperatriz Leopoldinense - 2017', '262', bounds);

		} // end if point
	
		
		old=new Array();
		 old = splitMapsCoord('-22.8955242,-43.19163850000001,13');		var lat = 0;
		var long = 0;
		var h = mapHeight;

		

		// get coordinates from the map field
		if (old.length>0) {
			lat = parseFloat(old[0]);
			long = parseFloat(old[1]);
			h = parseInt(old[2]);
		} 
		else 
		// try to get the coordinates using geocoding
		{
			var address = " Rio de Janeiro";

			if(address) {
			$.ajax({
				type		: "GET",
				cache		: false,
				url		: "http://www.guiadaboa.com.br/include/get_info.php?type=geocoding&address="+address,
				data		: $(this).serializeArray(),
				success: function(data) {
					if(data) {
 					var split_data=data.split("|");
						lat = split_data[0];
						long = split_data[1];

						// add the point to the map
						p = new google.maps.LatLng(lat , long);
						
						addPoint(p, 'Afoxé Filhos de Gandhi - 2017', '1964', bounds);
						
						bestFitWithCenter(listings_map, bounds);
					}
				}
			});
			}// end if address

		}

		if(lat && long) {	
		

			p = new google.maps.LatLng( lat , long );
			addPoint(p, 'Afoxé Filhos de Gandhi - 2017', '1964', bounds);

		} // end if point
	
		
		
		bestFitWithCenter(listings_map, bounds);

	}); // end bounds_changed

}

function bestFitWithCenter(map, bounds) {

	mapCenter = bounds.getCenter();
	var swLL = bounds.getSouthWest();
	var neLL = bounds.getNorthEast();
	//leave margin each side 
	var marginRatio = 0.0001;

	var minLat = Math.min(2*mapCenter.lat() - neLL.lat(), swLL.lat());
	var maxLat = Math.max(2*mapCenter.lat() - swLL.lat(),  neLL.lat());
	var minLng = Math.min(2*mapCenter.lng() - neLL.lng(), swLL.lng());
	var maxLng = Math.max(2*mapCenter.lng() - swLL.lng(), neLL.lng());

	var minLatLng = new google.maps.LatLng(minLat-marginRatio, minLng-marginRatio);
	var maxLatLng = new google.maps.LatLng(maxLat+marginRatio, maxLng+marginRatio);

	bounds.extend(maxLatLng);
	bounds.extend(minLatLng);

	map.setCenter(mapCenter);
	map.fitBounds(bounds);

}


function addPoint(point, listing_title, listing_id, bounds) {

	bounds.extend(point);
	var marker = new google.maps.Marker({
	      position: point,
	      title: listing_title
	});
	marker.setMap(listings_map);


	google.maps.event.addListener(marker, "click", function(){

	$.ajax({
		type		: "GET",
		cache		: false,
		url		: "http://www.guiadaboa.com.br/include/get_info.php?type=listing&id="+listing_id,
		data		: $(this).serializeArray(),
		success: function(data) {
			$.fancybox(data, { 'autoDimensions'	: false,
				'width'         		: 500,
				'height'        		: 'auto',
				'transitionIn'		: 'none',
				'transitionOut'		: 'none',
				'margin'		: '0',
				'padding'		: '0'
		});
		}
	});
			
        });

}





</script>		
