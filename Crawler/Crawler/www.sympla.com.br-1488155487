<script type="text/javascript">$.widget("custom.catcomplete",$.ui.autocomplete,{_create:function(){this._super();this.widget().menu("option","items","> :not(.ui-autocomplete-category)");},_renderItem:function(ul,item){return $("<li>").attr("data-value",item.value).append('<div class="ui-menu-item-wrapper">'+item.value+'</div>').appendTo(ul);},_renderMenu:function(ul,items){var that=this,currentCategory="";$.each(items,function(index,item){var li;if(item.category!=currentCategory){ul.append("<li class='ui-autocomplete-category'>"+item.category+"</li>");currentCategory=item.category;}
li=that._renderItemData(ul,item);if(item.category){li.attr("aria-label",item.category+" : "+item.value);}});}});function createCatCompete(){var data1=window.defaultCategories;$(document).on('click','#menu-icon-nav',function(){$(this).children().first().toggle();$(this).children().last().toggle();});$(document).on('shown.bs.collapse','#menu-navigation-collapsed',function(){$('body').addClass('noscroll');});$(document).on('hidden.bs.collapse','#menu-navigation-collapsed',function(){$('body').removeClass('noscroll');});$("#tags").on('focus',function(){$('.globalShadow').show();});$("#tags").on('blur',function(){$('.globalShadow').hide();});$("#tags").catcomplete({delay:0,appendTo:"#input-search-navbar",source:function(request,response){if((request.term||'').length==0)
response(data1);else if(request.term.length>2){$.ajax({url:"/search/suggestions?query="+request.term+"&only=events,cities",success:function(data){response(data);}});}},select:function(event,ui){window.location.assign(ui.item.url);},minLength:0}).bind('focus',function(){$(this).catcomplete("search");});}
$(document).ready(function(){createCatCompete();});function realoadNavbarWhite(){isGuest=false;$('#dialogLogin').modal('hide');$.ajax({'type':'POST','beforeSend':function(data){$('#my-nav').html('<i class="fa fa-spinner fa-2x fa-spin fa-pulse pull-right" style="margin-top:15px;"></i>');},'data':{'callback':'realoadNavbarWhite'},'dataType':'html','url':'/site/reloadNavbarWhiteMenu','cache':false,'success':function(html){$('#nav-home').html(html);createCatCompete();},'error':function(){$('#my-nav').html('');}});}</script>
