<script type='text/javascript'>

$(document).ready(function() {
  var nav = $('.filtrar_pesquisa');
         $(window).scroll(function () {
              if (($(this).scrollTop() > 4870)) {
                  nav.addClass("filtrar_pesquisa_fixo");
              } else {
                  nav.removeClass("filtrar_pesquisa_fixo");
              }
        });
 });
 
</script>



 




 


