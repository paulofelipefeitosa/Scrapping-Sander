<script type="text/javascript">var append_content=function(response){$('#events-grid').append(response.content);$('.spinner').hide();$('#more-events').show();if(response.next){$('#more-events').data('url',response.next);}else{$('#more-events').hide();}};var load_more=function(){var url=$(this).data('url');$('#more-events').hide();$('.spinner').show();$.getJSON(url,append_content);};$(document).ready(function(){$('#more-events').on('click',load_more);$(window.defaultCategories).each(function(index,el){$('#dropdownCategories:hidden').append('<li><a data-category='+el.value+'>'+el.label+'</a></li>');});$(".collapse").on('shown.bs.collapse',function(){$('body').addClass('noscroll');$('#nav-home').css('opacity',1);if($('#inputFullSearchHomeResp'))
$('#inputFullSearchHomeResp').focus();var refCat=$('body').find('#search-bar-collapsed')[0].childNodes[5];$("#inputCitySearchHomeResp").catcomplete({delay:0,appendTo:"#inputCitySearchHomeRespContainer",classes:{"ui-autocomplete":"responsive"},source:function(request,response){if(request.term.length>2){$.ajax({url:"/search/suggestions?query="+request.term+"&only=cities",success:function(data){response(data);}});}},response:function(event,ui){if(ui.content.length>0){dataToSearch.city=ui.content[0].label;$('#inputCitySearchHomeResp').data('value',ui.content[0].label);cityToSearch=ui.content[0].value;}
else{dataToSearch.city='';$('#inputCitySearchHomeResp').data('value','');cityToSearch='';}},select:function(event,ui){dataToSearch.city=ui.item.value;$('#inputCitySearchHomeResp').data('value',ui.item.label);},minLength:0}).bind('focus',function(){$(this).catcomplete("search");});});$(".collapse").on('hidden.bs.collapse',function(){$('body').removeClass('noscroll');$('#nav-home').css('opacity',0.9);$("#inputCitySearchHomeResp").catcomplete('destroy');});$('#dropdownMenuDates li a').on('click',function(){$('#searchForData').text($(this).text());dataToSearch.period=$(this).data('period');$('#searchForData').data('value',$(this).data('period'));});$('#dropdownMenuDatesResp li a').on('click',function(){$('#searchForDataResp').text($(this).text());dataToSearch.period=$(this).data('period');$('#searchForDataResp').data('value',$(this).data('period'));});var scrollPage=$(window).scrollTop();var searchBarOffset=$('#search-bar-collapsed-input').offset();if($(this).width()<767&&scrollPage>=searchBarOffset.top){$('#search-icon-nav').show();}
if($(this).width()<767&&scrollPage<searchBarOffset.top){$('#search-icon-nav').hide();}
$(window).scroll(function(){scrollPage=$(window).scrollTop();if($(this).width()<767&&scrollPage>=searchBarOffset.top){$('#search-icon-nav').show();}
if($(this).width()<767&&scrollPage<searchBarOffset.top){$('#search-icon-nav').hide();}});var dataToSearch={'full':'','city':'','period':'','category':''};$("#inputFullSearchHome").on('keyup',function(e){if(e.keyCode==13){dataToSearch.full=$(this).val();if($('#inputCitySearchHome').data('value'))
dataToSearch.city=$('#inputCitySearchHome').data('value');else{dataToSearch.city='';}
if($('#searchForData').data('value'))
dataToSearch.period=$('#searchForData').data('value');else{dataToSearch.period='';}
var url=buildUrl(dataToSearch);window.location.assign(url)}});var buildUrl=function(dataToSearch){var url=window.location.origin+'/eventos';if(dataToSearch.city!='')
url=url.concat('/'+dataToSearch.city);if(dataToSearch.category!='')
url=url.concat('/'+dataToSearch.category);if(dataToSearch.period!='')
url=url.concat('/'+dataToSearch.period);if(dataToSearch.full!='')
url=url.concat('?s='+dataToSearch.full);return url;}
var cityToSearch='';$('#inputCitySearchHome').on('blur',function(){if($(this).val().length<3)
cityToSearch='';$(this).val(cityToSearch);});$('#searchEventsByFilters').on('click',function(){dataToSearch.full=$('#inputFullSearchHome').val();if($('#inputCitySearchHome').data('value'))
dataToSearch.city=$('#inputCitySearchHome').data('value');else{dataToSearch.city='';}
if($('#searchForData').data('value'))
dataToSearch.period=$('#searchForData').data('value');else{dataToSearch.period='';}
var url=buildUrl(dataToSearch);window.open(url,'_self');});$('#searchEventsByFiltersResp').on('click',function(){dataToSearch.full=$('#inputFullSearchHomeResp').val();if($('#inputCitySearchHomeResp').data('value'))
dataToSearch.city=$('#inputCitySearchHomeResp').data('value');else{dataToSearch.city='';}
if($('#searchForDataResp').data('value'))
dataToSearch.period=$('#searchForDataResp').data('value');else{dataToSearch.period='';}
var url=buildUrl(dataToSearch);window.open(url,'_self');});$("#inputCitySearchHome").catcomplete({delay:0,appendTo:"#symplaSearchBarInput",source:function(request,response){if(request.term.length>2){$.ajax({url:"/search/suggestions?query="+request.term+"&only=cities",success:function(data){response(data);}});}},select:function(event,ui){dataToSearch.city=ui.item.label;$('#inputCitySearchHome').data('value',ui.item.label);},response:function(event,ui){if(ui.content.length>0){dataToSearch.city=ui.content[0].label;$('#inputCitySearchHome').data('value',ui.content[0].label);cityToSearch=ui.content[0].value;}
else{dataToSearch.city='';$('#inputCitySearchHome').data('value','');cityToSearch='';}},minLength:0}).bind('focus',function(){$(this).catcomplete("search");});$("#inputCitySearchHomeResp").catcomplete({delay:0,appendTo:"#inputCitySearchHomeRespContainer",source:function(request,response){if(request.term.length>2){$.ajax({url:"/search/suggestions?query="+request.term+"&only=cities",success:function(data){response(data);}});}},select:function(event,ui){dataToSearch.city=ui.item.value;$('#inputCitySearchHomeResp').data('value',ui.item.label);},response:function(event,ui){if(ui.content.length>0){dataToSearch.city=ui.content[0].label;$('#inputCitySearchHomeResp').data('value',ui.content[0].label);cityToSearch=ui.content[0].value;}
else{dataToSearch.city='';$('#inputCitySearchHomeResp').data('value','');cityToSearch='';}},minLength:0}).bind('focus',function(){$(this).catcomplete("search");});});</script>

