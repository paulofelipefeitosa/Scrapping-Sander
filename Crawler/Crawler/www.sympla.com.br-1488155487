<script type="text/javascript">var formValidador=new validator();formValidador.addField('fale-consultor','dialogContactOrgNome','O campo Nome é obrigatório.','required');formValidador.addField('fale-consultor','dialogContactOrgEmail','O e-mail informado não é válido.','email');formValidador.addField('fale-consultor','dialogContactOrgEmail','O e-mail informado não é válido.','required');formValidador.addField('fale-consultor','dialogContactOrgPhone','O campo Telefone é obrigatório.','required');formValidador.addField('fale-consultor','dialogContactOrgProdName','O campo Nome produtora/evento é obrigatório.','required');formValidador.addField('fale-consultor','dialogContactOrgMsg','O campo Mensagem é obrigatório.','required');function closeFaleConsultor()
{$('#dialog-fale-consultor').modal('hide');clearFaleConsultor();}
function showFaleConsultor(actionName)
{$("#divEventMsgCU").html('');$("#actionsContactOrg").show();$('#dialog-fale-consultor').modal('show');$('#hidCA').val(actionName);}
function sendEmailConsultor()
{if(formValidador.checkFormFields('fale-consultor','divEventMsgCU'))
{jQuery.ajax({'type':'POST','beforeSend':function(data){$("#actionsContactOrg").hide();$('#divEventMsgCU').html('<div><center><span><img alt=""  src="/images/img-loading.gif"></span></center></div>');},'data':{'email':$("#dialogContactOrgEmail").val(),'message':$("#dialogContactOrgMsg").val(),'name':$("#dialogContactOrgNome").val(),'cc':$("#hidEmail").val(),'prodname':$("#dialogContactOrgProdName").val(),'phone':$("#dialogContactOrgPhone").val(),'area':$("#dialogContactOrgProdArea").val(),'suser':$("#dialogContactSymplaUser").val(),'ca':$('#hidCA').val(),'urlreferer':''},'dataType':'json','url':'/site/sendEmailConsultant','cache':false,'success':function(data){if(data.status=="ok"){$("#divEventMsgCU").show().html('<img height="1" width="1" style="border-style:none;" alt="" src="//www.googleadservices.com/pagead/conversion/1025760016/?label=PP4fCPqvumUQkLaP6QM&amp;guid=ON&amp;script=0"/><img height="1" width="1" alt="" style="display:none" src="https://www.facebook.com/tr?ev=6045616710041&amp;cd[value]=0.00&amp;cd[currency]=BRL" /><div class="alert alert-success text-center">Sua mensagem foi enviada com sucesso!  <a href="javascript:closeFaleConsultor();">Fechar</a></div>');var fccEventCategory=$('#dialogContactOrgProdArea').val();var fccAlreadySells=$('#dialogContactSymplaUser').val();_kmq.push(['record','Submitted FcC',{'FCC Event Category':fccEventCategory,'FCC Already Sells Online':fccAlreadySells}]);fbq('track','Lead');}else{$("#divEventMsgCU").show().html("<div class=\"alert alert-danger\"><span>Ooops...</span></div>");$("#actionsContactOrg").show();$('#divEventMsgCU').html('');}},'error':function(){$("#divEventMsgCU").show().html("<div class=\"alert alert-danger\"><span>error-contect-general</span></div>");$("#actionsContactOrg").show();$('#divEventMsgCU').html('');}});}}
function clearFaleConsultor(){$('#dialog-fale-consultor form input, #dialog-fale-consultor form  textarea').val('');$('#dialog-fale-consultor form input, #dialog-fale-consultor form  textarea').css("background-color","");$('#divEventMsgCU').html('');}</script>

