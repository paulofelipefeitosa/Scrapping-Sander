<script type="text/javascript" charset="utf-8">
    (function (d, w) {
        jQuery('a[name=ic_fb_share]').click(function (e) {
            e.preventDefault();
            w.open(jQuery(this).attr('href'),'', 'height=350,width=650' );
        });
