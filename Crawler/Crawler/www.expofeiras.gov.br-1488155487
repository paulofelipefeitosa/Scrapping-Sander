<script>
    var year = '2017';
    var data = '';
    if(data != ''){
        data = data.substr(5);
    }

    $(function(){
        $( "#calAtual"). datepicker({
            dateFormat: 'mm-dd',
            /* DESENVOLVIMENTO: tradução dos dias */
            dayNamesMin: translate.dayNamesMin,
            onSelect: function(dateText, inst){
                //remove todos os inputs hiddens do form
                $('#buscaEvento').find('input:hidden').remove();
                $('#buscaEvento').find('input:not(:submit)').val('');
                $('#buscaEvento').append("<input type='hidden' name='data' value='"+ year + '-' + dateText +"'/>")
                $('#buscaEvento').submit();
                //Util.redirect('/calendario-de-eventos/index/data/' + year + '-' + dateText + '#calAtual') 
            },
            //defaultDate: ''
            defaultDate: data,
            /* DESENVOLVIMENTO: tradução dos meses */
            monthNames: translate.monthNames,
            monthNamesShort: translate.monthNamesShort
        });

        $( "#calProxAno"). datepicker({
            dateFormat: 'mm-dd',
            /* DESENVOLVIMENTO: tradução dos dias */
            dayNamesMin: translate.dayNamesMin,
            onSelect: function(dateText, inst){ Util.redirect('/calendario-de-eventos/index/data/' + (parseInt(year, 10) + 1) + '-' + dateText + '#calProxAno') },
            //defaultDate: ''
            defaultDate: data,
            /* DESENVOLVIMENTO: tradução dos meses */
            monthNames: translate.monthNames,
            monthNamesShort: translate.monthNamesShort
        });        
        $( "#calProxAno").datepicker( "setDate" , '+1y');
        
    })
</script>

