</script><script>
    $(function(){
        //Janela modal para o enviar email
    	$(".openEmailMsg2").fancybox({
    		'autoDimensions': true,
    		'transitionIn' : 'none',
    		'transitionOut' : 'none',
    		'titleShow' : false,
    		'onComplete': Enviar.enviarEmail.validar,
    		'onCleanup': Enviar.enviarEmail.reset
    	});

        $('a.icoGostei').click(function(){
            //guarda o obj elemento para ser usado na function de retorno
            var icoGostei = $(this);
            //faz requisicao ajax para incrementar e pegar o valor incrementado
            $.getJSON($(this).attr('href'), null, function(r){
                if(r.type == 'success'){
                    //em caso de sucesso atualiza o span
					var labelGostou = 'gostou deste evento';
					var labelGostaram = 'gostaram deste evento';
					if (r.nu_acessoweb > 1)
						icoGostei.parent().find('span').html(r.nu_acessoweb + ' ' + labelGostaram);
					else
						icoGostei.parent().find('span').html(r.nu_acessoweb + ' ' + labelGostou);
                }else{
                    //em caso de erro exibe a mensagem vinda do php
                    Modal.message('Erro', r.msg);
                }
            });
            return false;
        });

        $("#a_filtro_setores").toggle(abreFiltro,fechaFiltro);
        $("#a_filtro_estados").toggle(abreFiltro,fechaFiltro);

        //tira a seleção de outros estados
        //caso seja selecionado 'todos'
        $("#todos_e").click(function(){
            if($(this).is(':checked')){
                $("input[name^='estados']").removeAttr('checked');
                $("input[name^='estados']").attr('disabled', 'true');
            }else{
                $("input[name^='estados']").removeAttr('disabled');
            }
        });

        //seleciona todos os setores
        $("#todos_s").click(function(){
            if($(this).is(':checked')){
                $("input[name^='setores']").removeAttr('checked');
                $("input[name^='setores']").attr('disabled', 'true');
            }else{
                $("input[name^='setores']").removeAttr('disabled');
            }
        });

        $(".formFiltroSetores").submit(function(){
            var arrInputs = $(this).serializeArray();
            
            //remove todos os inputs hiddens do form
            $('#buscaEvento').find('input:hidden').remove();
            
            $.each(arrInputs, function(index, campo){
                $('#buscaEvento').append("<input type='hidden' name="+campo.name+"[] value='"+campo.value+"'/>")
            })
            $('#buscaEvento').submit();
            //var data = Util.urlZend($(this).serializeArray());
           // Util.redirect('/calendario-de-eventos/index'+data);
            return false;
        });

        paginacaoSemAjax();
    });

    function abreFiltro(){
        $(".filtrosCalendario:visible").hide();
        $("#"+$(this).attr('href')).show();
        return false;
    }

    function fechaFiltro(){
        $("#"+$(this).attr('href')).hide();
        return false;
    }
</script>


