 function getCleanWords(txt)
 {
 var w=(txt!=null?txt:'');
 while(w.indexOf('-')!=-1) w=w.replace('-',' ');
 return w;
 }

 var codtesteira = "453";

function buscaGeral()
{
var keywords= document.buscageral.keywords;
var txt_busca= document.busca.txt_busca;

keywords.value = getCleanWords(txt_busca.value);

if( codtesteira == "" )
{
document.buscageral.testeira.value="453";
}
 else
 {
 document.buscageral.testeira.value = codtesteira;
 }

if ( txt_busca.value.length >= 2 )
{
document.buscageral.submit();
}
else
{
alert("Para efetuar a busca digite no mínimo 2 letras.");
}
}

 var isHome=false;

var tipo = "";
if((tipo==null)||(tipo=="")){
tipo="NONE";
}

var newsIDValue = 'DYNAMIC,oracle.br.dataservers.ContentEventDataServer18,selectEvents';

if( newsIDValue=='0' ){
 isHome=true;
newsIDValue = ',F0';
}else{
newsIDValue = newsIDValue.substring(1,newsIDValue.indexOf(".htm"));
if(newsIDValue != '') newsIDValue = ",C" + newsIDValue;
}

var RCD_ADs_sitepage = 'T300' + newsIDValue;
var RCD_ADs_listpos = 'LE-Posicao01,LE-Posicao02,LE-Posicao03,LE-Posicao04,LE-Posicao05,LE-Posicao06,LE-Posicao07,LE-Posicao08,LE-Posicao09,LE-Posicao10,LE-HOME-Posicao01,LE-HOME-Posicao02,LE-HOME-Posicao03,LE-HOME-Posicao04,LE-HOME-Posicao05,LE-HOME-Posicao06,LE-HOME-Posicao07,LE-HOME-Posicao08,LE-HOME-Posicao09,LE-HOME-Posicao10';
var RCD_ADs_rn = new String(Math.random());
var RCD_ADs_rns = RCD_ADs_rn.substring(2);
var RCD_ADs_version = 10;

function RCD_NORMAL(position){}

RCD_ADs_version=11;
if(navigator.userAgent.indexOf('Mozilla/3')!=-1) RCD_ADs_version=10;
if(RCD_ADs_version==11){
document.write('<SCRIPT LANGUAGE=JavaScript1.1 SRC=' + '/jsp/senacADjsscript.jsp?sitepage=' + RCD_ADs_sitepage + '&extra=' + RCD_ADs_rns + '><\/SCRIPT>');
 }

 function RCD_AD(position){
 if(RCD_ADs_version==11) RCD_RICH(position);
 else RCD_NORMAL(position);
 }

