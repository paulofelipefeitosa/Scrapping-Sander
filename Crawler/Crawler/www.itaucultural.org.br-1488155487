<script type="text/javascript">
    jQuery('.c9sharer-link').click(function() {
        var rede = jQuery(this).attr('rede');
        var artigo = jQuery(this).attr('artigo');
        var original = jQuery(this).attr('url');
        var url = 'action=encurtar_url_ajax&url=' + original;
        var url_remota = false;
        $.ajax( {
            url: 'http://www.itaucultural.org.br/wp-admin/admin-ajax.php',
            dataType: 'JSON',
            type: 'GET',
            cache: false,
            data: url,
            success: function(dados) {
                switch(rede)
                {
                    case 'google':
                        var url_remota = 'https://plus.google.com/share?url=' + dados['short_url'];
                        break;
                    case 'twitter':
                        var url_remota = 'https://twitter.com/intent/tweet?original_referer=' + dados['url'] + '&source=tweetbutton&text=' + artigo + '&url=' + dados['short_url'];
                        break;
                    case 'facebook':
                        var url_remota = 'https://www.facebook.com/sharer/sharer.php?u=' + dados['short_url'];
                        break;
                }
                if(url_remota)
                {
                    window.open(url_remota, '', 'width=700,height=550');
                    // console.log(url_remota);
                }
            },
            error: function(error)
            {
                alert('Ocorreu uma falha de comunicação com o servidor.');
            }
        });
    });
</script>

